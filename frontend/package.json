{
  "name": "frontend2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build:production": "vite build --mode production",
    "preview": "vite preview",
    "detele-old-build": "rm -rf build && rm ./build.zip",
    "zip": "cd build; zip -r ../build.zip *; cd ../",
    "all": "yarn detele-old-build; yarn build:production; yarn zip; yarn scp",
    "scp": "scp build.zip root@161.35.109.81:/var/www/html/dunitapp",
    "unzip": "ssh root@161.35.109.81 \"cd /var/www/html/dunitapp && find . \\! -name 'build.zip' -delete && unzip build.zip && rm build.zip\""
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bootstrap": "^5.3.1",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-qr-reader": "^3.0.0-beta-1",
    "react-toastify": "^9.1.3",
    "styled-components": "^6.0.0-rc.3",
    "util": "^0.12.5",
    "web3": "^4.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-modal": "^3.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "browserify-zlib": "^0.2.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "react-router-dom": "^6.13.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  }
